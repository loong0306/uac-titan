<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="${dubbo.application.name}" />
    <!-- 注册中心服务地址 -->
    <dubbo:registry client="${dubbo.registry.client}" protocol="${dubbo.registry.protocol}" address="${dubbo.registry.address}" />
    <!-- 服务配置 -->
    <dubbo:protocol name="${dubbo.protocol.name}" port="${dubbo.protocol.port}" />
    <!-- SERVICE -->
    <!-- 生产服务 -->
    <!-- UAC平台信息 -->
    <dubbo:service interface="me.uac.service.dubbo.UacDubboPlatformService"
                   ref="uacDubboPlatformServiceImpl" retries="0" timeout="6000" />
    <bean id="uacDubboPlatformServiceImpl" class="me.uac.service.dubbo.impl.UacDubboPlatformServiceImpl"/>
</beans>